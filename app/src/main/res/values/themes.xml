<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <!--不带项目名字的主题-->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorOriginal</item>
        <item name="colorPrimaryDark">@color/colorOriginalDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.XierHelper" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorOriginal</item>
        <item name="colorPrimaryVariant">@color/colorOriginalDark</item>
        <item name="colorOnPrimary">@color/colorAccent</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>